{
  "name": "sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "prepare": "svelte-kit sync && yarn smui-theme",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0",
    "@smui/common": "^6.0.0-beta.16",
    "@smui/fab": "^6.0.0-beta.16",
    "@smui/list": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@smui/top-app-bar": "^6.0.0-beta.16",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "1.0.0-next.380",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.46.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0"
  },
  "type": "module"
}
